[color]
  ui = true
[user]
  name = misoti 
  email = dev@misogi.info
[alias]
  st = status -sb
  up = pull --rebase
[push]
  default = tracking
[core]
  editor = vim
[diff]
  algorithm = histogram
[help]
	autocorrect = 0
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[fetch]
	prune = true
